{
  "compilerOptions": {
    "lib": [
      "dom",
      "dom.iterable",
      "dom.extras",
      "deno.ns"
    ],
    "types": [
      "https://deno.land/x/aleph@1.0.0-beta.38/types.d.ts"
    ]
  },
  "tasks": {
    "dev": "deno run -A apps/spinnybox_com/dev.ts",
    "start": "deno run -A apps/spinnybox_com/server.ts",
    "build": "deno run -A apps/spinnybox_com/server.ts --build",
    "esm:add": "deno run -A https://esm.sh add",
    "esm:update": "deno run -A https://esm.sh update",
    "esm:remove": "deno run -A https://esm.sh remove"
  },
  "imports": {
    "std/": "https://deno.land/std@0.178.0/",
    "@unocss/core": "https://esm.sh/@unocss/core@0.50.4",
    "@unocss/preset-mini": "https://esm.sh/@unocss/preset-mini@0.50.4",
    "@unocss/preset-typography": "https://esm.sh/@unocss/preset-typography@0.50.4",
    "@unocss/preset-uno": "https://esm.sh/@unocss/preset-uno@0.50.4",
    "unocss-preset-theme": "https://esm.sh/unocss-preset-theme@0.5.0",
    "aleph/": "https://deno.land/x/aleph@1.0.0-beta.38/",
    "aleph/server": "https://deno.land/x/aleph@1.0.0-beta.38/server/mod.ts",
    "aleph/dev": "https://deno.land/x/aleph@1.0.0-beta.38/server/dev.ts",
    "aleph/unocss": "https://deno.land/x/aleph@1.0.0-beta.38/server/unocss.ts"
  }
}
